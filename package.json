{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build_main": "NODE_ENV=development && nuxt build --dotenv .env.development",
    "build_release": "nuxt build --dotenv .env.production",
    "production": "nuxt build --dotenv .env.production",
    "dev": "nuxt dev --dotenv .env.development",
    "type-check": "nuxt typecheck",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test:unit": "vitest --run",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "precommit": "pnpm lint && pnpm type-check && pnpm build && pnpm test:unit",
    "clean": "pnpm lint:fix && pnpm type-check && pnpm run build && pnpm run test:unit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@appwise/eslint-config-vue": "^1.2.3",
    "@appwise/oauth2-client": "^0.0.4",
    "@appwise/zod-http-client": "^0.0.4",
    "@headlessui-float/vue": "^0.13.2",
    "@tanstack/vue-query": "^5.24.1",
    "@vuepic/vue-datepicker": "^8.1.1",
    "@vueuse/core": "^10.8.0",
    "@vueuse/router": "^10.8.0",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "cva": "1.0.0-beta.1",
    "dayjs": "^1.11.10",
    "focus-trap-vue": "^4.0.3",
    "formango": "^2.0.11",
    "nuxt": "^3.10.3",
    "tailwind-merge": "^2.2.1",
    "vue": "^3.4.20",
    "vue-router": "^4.2.5",
    "vue-sonner": "^1.0.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.11.0",
    "@nuxtjs/i18n": "^8.1.1",
    "@pinia/nuxt": "^0.5.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tanstack/vue-query-devtools": "^5.24.1",
    "@vitest/ui": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-plugin-require-explicit-generics": "^1.0.0",
    "eslint-plugin-tailwindcss": "^3.14.3",
    "happy-dom": "^13.6.2",
    "husky": "^9.0.11",
    "playwright-core": "^1.41.2",
    "postcss": "^8.4.35",
    "sass": "^1.71.1",
    "tailwind": "^4.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  }
}
